{
  "name": "seo-lint",
  "version": "0.1.0",
  "description": "SEO Lint - The package for linting your HTML file with Google SEO",
  "main": "lib/index.js",
  "repository": "git@github.com:lucduong/seo-lint.git",
  "author": "Luc <luc@ltv.vn>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "test": "mocha --reporter spec",
    "lint": "eslint --ext=.js src",
    "lint:fix": "eslint --fix --ext=.js src",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --require babel-core/register -R spec test/*.js"
  },
  "bin": {
    "seolint": "bin/seolint"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "clui": "^0.3.6",
    "commander": "^2.14.1",
    "figlet": "^1.2.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.17.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0"
  }
}
